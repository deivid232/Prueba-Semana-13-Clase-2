<div class="row">
  <div class="col-md-6">
    <% @todos.each do |todo| %>
      <% if todo.completed == true%>
      <h1> <%= todo.description%> </h1>
      <p> <%= todo.completed %> </p>
      <% end %>
    <% end %>
  </div>
  <div class="col-md-6">
    <% @todos.each do |todo| %>
      <% if todo.completed == false %>
      <h1> <%= todo.description%> </h1>
      <p> <%= todo.completed %> </p>
      <% end %>
    <% end %>
  </div>

</div>
