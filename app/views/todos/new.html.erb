<%= form_with model: @todo, local: true do |f| %>
      <!-- Es el titulo de title -->
    <%= f.label :description, "Descripcion" %>
    <!-- Escribo la info -->
    <%= f.text_field :description %>
    <!-- Es el titulo de content -->
    <%= f.label :completed, "Numero" %>
    <!-- Escribo la info -->
    <%= f.number_field :completed %>
    <!-- Coloca el boton -->
    <%= f.submit  "Enviar" %>
  


<% end %>
